{
  "name": "arnelirobles-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio site with rnxJS reactive UI",
  "private": true,
  "scripts": {
    "build:js": "esbuild assets/js/app.js --bundle --minify --outfile=assets/js/bundle.js --format=iife --global-name=App",
    "watch:js": "esbuild assets/js/app.js --bundle --watch --outfile=assets/js/bundle.js --format=iife --global-name=App",
    "dev": "npm run watch:js"
  },
  "dependencies": {
    "@arnelirobles/rnxjs": "latest"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  },
  "keywords": [
    "portfolio",
    "reactive",
    "rnxjs"
  ],
  "author": "Arnel Irobles",
  "license": "MIT"
}
